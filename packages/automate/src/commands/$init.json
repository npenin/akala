{
    "$schema": "https://raw.githubusercontent.com/npenin/akala/master/packages/commands/command-schema.json",
    "": {
        "inject": [
            "param.0"
        ]
    },
    "cli": {
        "usage": "$init [persist]",
        "inject": [
            "options.persist"
        ]
    },
    "fs": {
        "path": "$init.js",
        "source": "../../src/commands/$init.ts",
        "inject": [
            "param.0"
        ]
    }
}