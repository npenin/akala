{
    "$schema": "https://raw.githubusercontent.com/npenin/akala/master/packages/commands/command-schema.json",
    "": {
        "inject": [
            "$container",
            "param.0",
            "param.1"
        ]
    },
    "fs": {
        "inject": [
            "$container",
            "param.0",
            "param.1"
        ]
    },
    "cli": {
        "usage": "load <name> <file>",
        "inject": [
            "dummy",
            "options.name",
            "options.file"
        ],
        "options": {
            "file": {
                "normalize": "require"
            }
        }
    }
}