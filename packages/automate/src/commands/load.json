{
    "$schema": "https://raw.githubusercontent.com/npenin/akala/main/packages/commands/command-schema.json",
    "": {
        "inject": [
            "$container",
            "params.0",
            "params.1"
        ]
    },
    "fs": {
        "inject": [
            "$container",
            "params.0",
            "params.1"
        ]
    },
    "cli": {
        "usage": "load <name> <file>",
        "inject": [
            "dummy",
            "options.name",
            "options.file"
        ],
        "options": {
            "file": {
                "normalize": true
            }
        }
    }
}
