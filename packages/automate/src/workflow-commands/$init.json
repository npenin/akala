{
    "$schema": "https://raw.githubusercontent.com/npenin/akala/main/packages/commands/command-schema.json",
    "": {
        "inject": [
            "param.0",
            "pm",
            "param.1"
        ]
    },
    "cli": {
        "inject": [
            "options.name",
            "pm",
            "context"
        ]
    },
    "fs": {
        "path": "dist/workflow-commands/$init.js",
        "source": "src/workflow-commands/$init.ts",
        "inject": [
            "param.0",
            "pm",
            "param.1"
        ]
    }
}