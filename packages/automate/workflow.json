                    "inject": [
                        "options.name",
                        "pm",
                        "context"
                    ]
                }
            }
        },
        {
            "name": "process",
            "inject": [
                "param.0"
            ],
            "config": {
                "": {
                    "inject": [
                        "param.0"
                    ]
                },
                "fs": {
                    "path": "dist/workflow-commands/process.js",
                    "source": "src/workflow-commands/process.ts",
                    "inject": [
                        "param.0"
                    ]
                }
            }
        },
        {
            "name": "set-config",
            "inject": [
                "param.0"
            ],
            "config": {
                "": {
                    "inject": [
                        "param.0"
                    ]
                },
                "fs": {
                    "path": "dist/workflow-commands/set-config.js",
                    "source": "src/workflow-commands/set-config.ts",
                    "inject": [
                        "param.0"
                    ]
                }
            }
        },
        {
            "name": "use",
            "inject": [
                "param.0",
                "param.1",
                "param.2"
            ],
            "config": {
                "": {
                    "inject": [
                        "param.0",
                        "param.1",
                        "param.2"
                    ]
                },
                "fs": {
                    "path": "dist/workflow-commands/use.js",
                    "source": "src/workflow-commands/use.ts",
                    "inject": [
                        "param.0",
                        "param.1",
                        "param.2"
                    ]
                }
