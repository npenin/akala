                                        "conditions": [],
                                        "endpoint": {
                                            "url": "https://eks-auth.{Region}.{PartitionResult#dualStackDnsSuffix}",
                                            "properties": {},
                                            "headers": {}
                                        },
                                        "type": "endpoint"
                                    }
                                ],
                                "type": "tree"
                            },
                            {
                                "conditions": [
                                    {
                                        "fn": "booleanEquals",
                                        "argv": [
                                            {
                                                "ref": "UseFIPS"
                                            },
                                            true
