{
    "name": "@akala/core",
    "version": "41.11.0",
    "description": "",
    "scripts": {
        "test": "node --test --enable-source-maps dist/esm/test/**/*.js",
        "coverage": "node --test --enable-source-maps --experimental-test-coverage --test-reporter=lcov --test-reporter-destination=lcov.info --test-reporter=spec --test-reporter-destination=stdout dist/esm/test/**/*.js",
        "prepack": "../../makeCjs.sh",
        "build": "tsc -p tsconfig.esm.json"
    },
    "type": "module",
    "exports": {
        ".": {
            "browser": {
                "types": "./dist/esm/index.browser.d.ts",
                "default": "./dist/esm/index.browser.js"
            },
            "import": {
                "browser": "./dist/esm/index.browser.js",
                "types": "./dist/esm/index.d.ts",
                "bun": "./src/index.ts",
                "default": "./dist/esm/index.js"
            },
            "require": {
                "types": "./dist/cjs/index.d.ts",
                "bun": "./src/index.ts",
                "default": "./dist/cjs/index.js"
            }
        },
        "./expressions": {
            "import": {
                "types": "./dist/esm/parser/expressions/index.d.ts",
                "bun": "./src/parser/expressions/index.ts",
                "default": "./dist/esm/parser/expressions/index.js"
            },
            "require": {
                "types": "./dist/cjs/parser/expressions/index.d.ts",
                "bun": "./src/parser/expressions/index.ts",
                "default": "./dist/cjs/parser/expressions/index.js"
            }
        }
    },
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "publishConfig": {
        "provenance": false
    },
    "funding": "https://github.com/npenin/akala?sponsor=1",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/npenin/akala.git"
    },
    "homepage": "https://akala.js.org/core",
    "author": "npenin",
    "license": "BSD-3-Clause",
    "source": [
        "src/index.ts",
        "src/parser/expressions/index.ts"
    ],
    "devDependencies": {
        "@types/debug": "^4.1.12",
        "merge2": "^1.4.1",
        "typescript": "^5.8.3"
    },
    "types": "./dist/esm/index.d.ts",
    "dependencies": {
        "debug": "^4.4.1",
        "reflect-metadata": "^0.2.2",
        "yoctocolors": "^2.1.1"
    },
    "gitHead": "d92f6693ed8cca439c8e92d2045bb15c0a7dae43",
    "typings": "./dist/esm/index.d.ts"
}
