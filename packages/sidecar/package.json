{
    "name": "@akala/sidecar",
    "packageManager": "yarn@3.3.1",
    "main": "dist/index.js",
    "commands": "./commands.json",
    "version": "2.1.1",
    "type": "module",
    "dependencies": {
        "@akala/commands": "workspace:^",
        "@akala/config": "workspace:^",
        "@akala/pm": "workspace:^"
