{
    "$schema": "https://raw.githubusercontent.com/npenin/akala/main/packages/commands/command-schema.json",
    "fs": {
        "inject": [
            "param.0",
            "param.1",
            "param.2"
        ]
    },
    "cli": {
        "usage": "start [config] [remote]",
        "inject": [
            "context",
            "options.config",
            "options.remote"
        ],
        "options": {
            "config": {
                "aliases": [
                    "c"
                ],
                "normalize": true,
                "needsValue": true
            },
            "remote": {
                "needsValue": true
            }
        }
    },
    "": {
        "inject": [
            "param.0",
            "param.1",
            "param.2"
        ]
    }
}