{
    "name": "@akala/sidecar",
    "commands": [
        {
            "name": "$init",
            "config": {
                "fs": {
                    "inject": [
                        "param.0",
                        "param.1",
                        "param.2"
                    ],
                    "path": "dist/esm/commands/$init.js",
                    "source": "src/commands/$init.ts"
                },
                "cli": {
                    "usage": "start [config] [remote]",
                    "inject": [
                        "context",
                        "options.config",
                        "options.remote"
                    ],
                    "options": {
                        "config": {
                            "aliases": [
                                "c"
                            ],
                            "normalize": true,
                            "needsValue": true
                        },
                        "remote": {
                            "needsValue": true
                        }
                    }
                },
                "": {
                    "inject": [
                        "param.0",
                        "param.1",
                        "param.2"
                    ]
                }
            }
        }
    ],
    "$schema": "https://raw.githubusercontent.com/npenin/akala/master/packages/commands/container-schema.json"
}