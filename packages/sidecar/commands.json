{
    "name": "@akala/sidecar",
    "commands": [
        {
            "name": "$init",
            "inject": [],
            "config": {
                "fs": {
                    "inject": [
                        "param.0"
                    ],
                    "path": "dist/commands/$init.js",
                    "source": "src/commands/$init.ts"
                },
                "cli": {
                    "usage": "start [config] [remote]",
                    "inject": [
                        "options.config"
                    ],
                    "options": {
                        "config": {
                            "aliases": [
                                "c"
                            ],
                            "normalize": true,
                            "needsValue": true
                        }
                    }
                },
                "": {
                    "inject": [
                        "param.0"
                    ]
                }
            }
        }
    ],
    "$schema": "https://raw.githubusercontent.com/npenin/akala-commands/master/schema.json"
}