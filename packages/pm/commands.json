        },
        {
            "name": "map",
            "config": {
                "fs": {
                    "path": "dist/esm/commands/map.js",
                    "source": "src/commands/map.ts",
                    "inject": [
                        "param.0",
                        "param.1",
                        "param.2",
                        "param.3"
                    ]
                },
                "": {
                    "inject": [
                        "param.0",
                        "param.1",
                        "param.2",
                        "param.3"
                    ]
                },
                "cli": {
                    "inject": [
                        "options.name",
                        "options.path",
                        "cwd",
                        "options"
                    ],
                    "options": {
                        "commandable": {
                            "aliases": [
                                "c"
                            ],
                            "needsValue": false
                        },
                        "stateless": {
                            "aliases": [
                                "s"
                            ],
                            "needsValue": false
                        }
                    },
                    "usage": "map <name> <path>"
                }
            }
        },
        {
            "name": "name",
            "config": {
                "fs": {
                    "path": "dist/esm/commands/name.js",
                    "source": "src/commands/name.ts",
                    "inject": [
                        "param.0"
                    ]
                },
                "": {
                    "inject": [
                        "param.0"
                    ]
                }
            }
        },
        {
            "name": "plugin.add",
            "config": {
                "fs": {
                    "inject": [
