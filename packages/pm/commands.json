                        "param.0",
                        "param.1"
                    ]
                },
                "cli": {
                    "usage": "connect <name>",
                    "inject": [
                        "options.name",
                        "context"
                    ],
                    "options": {
                        "tcpPort": {
                            "needsValue": true,
                            "aliases": [
                                "tcp-port"
                            ]
                        },
                        "port": {
                            "needsValue": true
                        },
                        "key": {
                            "needsValue": true
                        },
                        "cert": {
                            "needsValue": true
                        }
                    }
                }
            }
        },
        {
            "name": "discover",
