                },
                "": {
                    "inject": [
                        "param.0",
                        "param.1",
                        "$container"
                    ]
                },
                "cli": {
                    "usage": "discover <name> <folder>",
                    "options": {
                        "folder": {
                            "normalize": true
                        }
                    },
                    "inject": [
                        "options.name",
                        "options.folder",
                        "$container"
                    ]
                }
            }
        },
