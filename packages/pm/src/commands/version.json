{
    "$schema": "https://raw.githubusercontent.com/npenin/akala/main/packages/commands/command-schema.json",
    "": {
        "inject": [
            "param.0",
            "param.1"
        ]
    },
    "cli": {
        "inject": [
            "options.packageName",
            "options.folder"
        ],
        "usage": "version <packageName> [folder]"
    },
    "schema": {
        "inject": [
            "param.0",
            "param.1"
        ],
        "$defs": {
            "param.0": {
                "type": "string"
            },
            "param.1": {
                "type": "string"
            }
        }
    }
}