{
    "$schema": "https://raw.githubusercontent.com/npenin/akala/master/packages/commands/command-schema.json",
    "": {
        "inject": [
            "$container",
            "param.0"
        ]
    },
    "cli": {
        "inject": [
            "$container",
            "context"
        ],
        "usage": "$init [config]",
        "options": {
            "config": {
                "needsValue": true,
                "aliases": [
                    "c"
                ]
            }
        }
    }
}