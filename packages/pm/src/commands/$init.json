{
    "$schema": "https://raw.githubusercontent.com/npenin/akala/master/packages/commands/command-schema.json",
    "": {
        "inject": [
            "$container",
            "param.0"
        ]
    },
    "cli": {
        "inject": [
            "$container",
            "context"
        ],
        "options": {
            "tcpPort": {
                "needsValue": true
            },
            "port": {
                "needsValue": true
            },
            "key": {
                "needsValue": true
            },
            "cert": {
                "needsValue": true
            }
        }
    }
}