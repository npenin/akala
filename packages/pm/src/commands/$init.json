{
    "$schema": "https://raw.githubusercontent.com/npenin/akala/main/packages/commands/command-schema.json",
    "fs": {
        "disabled": true
    },
    "": {
        "inject": [
            "$container",
            "param.0"
        ]
    },
    "cli": {
        "inject": [
            "$container",
            "context"
        ],
        "usage": "$init [...args]",
        "options": {
            "configFile": {
                "needsValue": true,
                "aliases": [
                    "c"
                ]
            },
            "keepAttached": {
                "needsValue": true
            }
        }
    }
}