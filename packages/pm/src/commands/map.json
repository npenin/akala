{
    "$schema": "https://raw.githubusercontent.com/npenin/akala/master/packages/commands/command-schema.json",
    "": {
        "inject": [
            "param.0",
            "param.1",
            "param.2",
            "param.3",
            "param.4"
        ]
    },
    "cli": {
        "inject": [
            "param.0",
            "param.1",
            "cwd",
            "options.c",
            "options.s"
        ],
        "options": {
            "commandable": {
                "aliases": [
                    "c"
                ],
                "needsValue": false
            },
            "stateless": {
                "aliases": [
                    "s",
                    "state-less"
                ],
                "needsValue": false
            }
        }
    }
}