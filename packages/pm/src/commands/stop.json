{
    "$schema": "https://raw.githubusercontent.com/npenin/akala/master/packages/commands/command-schema.json",
    "": {
        "inject": [
            "param.0",
            "$container"
        ]
    },
    "cli": {
        "inject": [
            "options.process",
            "$container"
        ],
        "options": {
            "process": {
                "doc": "process to stop. Stops all the processes otherwise (including pm)."
            }
        },
        "usage": "stop [process]"
    },
    "html-form": {
        "inject": [
            "param.0",
            "$container"
        ]
    },
    "schema": {
        "inject": [
            "param.0",
            "$container"
        ],
        "schema": {
            "$defs": {
                "param.0": {
                    "type": "string"
                }
            }
        }
    }
}