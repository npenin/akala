{
    "": {
        "inject": [
            "params.0",
            "params.1"
        ]
    },
    "cli": {
        "usage": "connect <name>",
        "inject": [
            "options.name",
            "context"
        ],
        "options": {
            "tcpPort": {
                "needsValue": true,
                "aliases": [
                    "tcp-port"
                ]
            },
            "port": {
                "needsValue": true
            },
            "key": {
                "needsValue": true
            },
            "cert": {
                "needsValue": true
            }
        }
    },
    "$schema": "https://raw.githubusercontent.com/npenin/akala/main/packages/commands/command-schema.json"
}
