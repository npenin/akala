{
    "$schema": "https://raw.githubusercontent.com/npenin/akala/main/packages/commands/command-schema.json",
    "": {
        "inject": [
            "params.0",
            "params.1",
            "$container"
        ]
    },
    "cli": {
        "usage": "discover <name> <folder>",
        "options": {
            "folder": {
                "normalize": true
            }
        },
        "inject": [
            "options.name",
            "options.folder",
            "$container"
        ]
    }
}
