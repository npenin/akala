{
    "name": "@akala/commands",
    "version": "3.9.3",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": "https://github.com/npenin/akala",
    "homepage": "https://akala.js.org/commands",
    "author": "npenin",
    "license": "BSD-3-Clause",
    "funding": "https://github.com/npenin/akala?sponsor=1",
    "scripts": {
        "test": "mocha",
        "prepack": "tsc -p .",
        "build": "tsc -p .",
        "cli": "node dist/cli.js",
        "generate-cli": "ac generate dist/cli commands.json --name cli",
        "generate-metadata-cli": "ac generate-metadata commands.json src/commands.ts --name cli",
        "generate": "yarn run generate-cli",
        "generate-metadata": "yarn run generate-metadata-cli"
    },
    "browser": {
        "./dist/cli/serve.js": false,
        "./dist/processors/fs.js": "./dist/processors/fs-browser.js",
        "./dist/serve-metadata.js": "./dist/serve-metadata.browser.js"
    },
    "bin": {
        "ac": "dist/cli.js"
    },
    "mocha": {
        "require": [
            "source-map-support/register"
        ],
        "spec": [
            "dist/test/*.js"
        ],
        "timeout": 0
    },
    "nyc": {
        "reporter": [
            "text-summary"
        ],
        "instrument": true
    },
    "dependencies": {
        "@akala/cli": "workspace:^",
        "@akala/core": "workspace:^",
        "@akala/json-rpc-ws": "workspace:^",
        "debug": "^4.3.4",
        "path-to-regexp": "^6.2.1",
        "reflect-metadata": "^0.1.13",
        "ws": "^8.9.0"
    },
    "devDependencies": {
        "@types/debug": "^4.1.7",
        "@types/mocha": "^9.1.1",
        "@types/node": "^18.7.22",
        "@types/split2": "^3.2.1",
        "@types/uuid": "^8.3.4",
        "@types/ws": "^8.5.3",
        "mocha": "^10.0.0",
        "nyc": "^15.1.0",
        "source-map-support": "^0.5.21",
        "typescript": "^4.8.3"
    },
    "gitHead": "d92f6693ed8cca439c8e92d2045bb15c0a7dae43"
}
