{
    "name": "@akala/commands",
    "version": "2.6.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": "https://github.com/npenin/akala",
    "homepage": "https://akala.js.org/commands",
    "author": "npenin",
    "license": "BSD-3-Clause",
    "funding": "https://github.com/npenin/akala?sponsor=1",
    "scripts": {
        "test": "mocha",
        "prepack": "tsc -p .",
        "build": "tsc -p .",
        "cli": "node dist/cli.js",
        "generate-cli": "ac generate dist/cli commands.json --name cli",
        "generate-metadata-cli": "ac generate-metadata commands.json src/commands.ts --name cli",
        "generate": "ac generate dist/cli commands.json --name @akala/commands"
    },
    "browser": {
        "./dist/cli/serve.js": false,
        "./dist/processors/fs.js": "./dist/processors/fs-browser.js",
        "./dist/serve-metadata.js": "./dist/serve-metadata.browser.js"
    },
    "bin": {
        "ac": "dist/cli.js"
    },
    "mocha": {
        "require": [
            "source-map-support/register"
        ],
        "spec": [
            "dist/test/*.js"
        ],
        "timeout": 0
    },
    "nyc": {
        "reporter": [
            "text-summary"
        ],
        "instrument": true
    },
    "dependencies": {
        "@akala/cli": "workspace:*",
        "@akala/core": "workspace:*",
        "@akala/json-rpc-ws": "workspace:*",
        "debug": "^4.1.1",
        "path-to-regexp": "^6.1.0",
        "reflect-metadata": "^0.1.13",
        "ws": "^8.0.0"
    },
    "devDependencies": {
        "@types/debug": "^4.1.6",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.11.9",
        "@types/split2": "^3.2.0",
        "@types/uuid": "^8.3.1",
        "@types/ws": "^7.4.6",
        "express": "^4.17.1",
        "mocha": "^9.0.2",
        "nyc": "^15.1.0",
        "source-map-support": "^0.5.19",
        "typescript": "^4.4.0"
    },
    "gitHead": "d92f6693ed8cca439c8e92d2045bb15c0a7dae43"
}
