{
    "$schema": "https://raw.githubusercontent.com/npenin/akala/master/packages/commands/command-schema.json",
    "cli": {
        "usage": "new <action> <name> [destination]",
        "options": {
            "force": {
                "aliases": [
                    "f"
                ]
            }
        },
        "inject": [
            "options.action",
            "options.name",
            "options",
            "options.destination"
        ]
    }
}