{
    "$schema": "https://raw.githubusercontent.com/npenin/akala/master/packages/commands/command-schema.json",
    "cli": {
        "inject": [
            "options.commandPath",
            "options.name",
            "options.output",
            "options"
        ],
        "usage": "generate-schema [commandPath] [output]",
        "options": {
            "name": {
                "needsValue": true
            }
        }
    }
}