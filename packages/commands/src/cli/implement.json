{
    "$schema": "https://raw.githubusercontent.com/npenin/akala/master/packages/commands/command-schema.json",
    "": {
        "inject": [
            "param.0",
            "param.1"
        ]
    },
    "cli": {
        "usage": "implement <commands> <destination>",
        "inject": [
            "options.commands",
            "options.destination",
            "options"
        ],
        "options": {
            "destination": {
                "normalize": true
            },
            "force": {
                "aliases": [
                    "f"
                ]
            }
        }
    }
}