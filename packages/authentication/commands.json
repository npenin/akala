                "fs": {
                    "inject": [
                        "param.0",
                        "param.1"
                    ],
                    "path": "dist/esm/server/commands/login.js",
                    "source": "src/server/commands/login.ts"
                },
                "http": {
                    "inject": [],
                    "route": "/login",
                    "method": "post"
                },
                "": {
                    "inject": [
                        "param.0",
                        "param.1"
                    ]
                },
                "html": {
                    "inject": [
                        "form.username",
                        "form.password"
                    ]
                }
            }
