{
    "$schema": "https://raw.githubusercontent.com/npenin/akala/main/packages/commands/command-schema.json",
    "http": {
        "inject": [
            "body.username",
            "body.password",
            "body.deviceId"
        ],
        "route": "/login",
        "method": "post"
    },
    "jsonrpc": {
        "inject": [
            "param.0",
            "param.1",
            "param.2",
            "connectionId"
        ]
    },
    "": {
        "inject": [
            "param.0",
            "param.1",
            "param.2"
        ]
    },
    "html": {
        "inject": [
            "form.username",
            "form.password",
            "form.deviceId"
        ]
    }
}