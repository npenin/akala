{
    "name": "@akala/server",
    "commands": [
        {
            "name": "$init",
            "inject": [],
            "config": {
                "fs": {
                    "path": "dist/commands/$init.js",
                    "source": "src/commands/$init.ts",
                    "inject": []
                },
                "cli": {
                    "inject": [
                        "$container",
                        "options",
                        "pm"
                    ],
                    "options": {
                        "mode": {
                            "needsValue": true
                        }
                    }
                },
                "": {
                    "inject": []
                }
            }
        },
        {
            "name": "mode",
            "inject": [],
            "config": {
                "fs": {
                    "path": "dist/commands/mode.js",
                    "source": "src/commands/mode.ts",
                    "inject": [
                        "param.0"
                    ]
                },
                "": {
                    "inject": []
                }
            }
        },
        {
            "name": "remote-container",
            "inject": [],
            "config": {
                "fs": {
                    "inject": [
                        "dummy",
                        "param.1",
                        "param.2"
                    ],
                    "path": "dist/commands/remote-container.js",
                    "source": "src/commands/remote-container.ts"
                },
                "jsonrpc": {
                    "inject": [
                        "connectionAsContainer",
                        "param.0",
                        "param.1"
                    ]
                },
                "": {
                    "inject": [
                        "param.0",
                        "param.1",
                        "param.2"
                    ]
                }
            }
        },
        {
            "name": "asset",
            "inject": [],
            "config": {
                "fs": {
                    "path": "dist/commands/asset.js",
                    "source": "src/commands/asset.ts",
                    "inject": [
                        "$container",
                        "param.0",
                        "param.1",
                        "ignore"
                    ]
                },
                "": {
                    "inject": [
                        "$container",
                        "param.0",
                        "param.1",
                        "cwd"
                    ]
                },
                "cli": {
                    "options": {
                        "normalize": [
                            "param.1"
                        ]
                    },
                    "inject": [
                        "$container",
                        "param.0",
                        "param.1",
                        "cwd"
                    ]
                }
            }
        },
        {
            "name": "remote-route",
            "inject": [],
            "config": {
                "fs": {
                    "path": "dist/commands/remote-route.js",
                    "source": "src/commands/remote-route.ts",
                    "inject": [
                        "param.0",
                        "param.1",
                        "param.2"
                    ]
                },
                "": {
                    "inject": []
                }
            }
        },
        {
            "name": "remove-asset",
            "inject": [],
            "config": {
                "fs": {
                    "path": "dist/commands/remove-asset.js",
                    "source": "src/commands/remove-asset.ts",
                    "inject": [
                        "param.0",
                        "param.1"
                    ]
                },
                "": {
                    "inject": []
                }
            }
        },
        {
            "name": "route",
            "inject": [],
            "config": {
                "fs": {
                    "path": "dist/commands/route.js",
                    "source": "src/commands/route.ts",
                    "inject": [
                        "param.0",
                        "param.1",
                        "param.2",
                        "param.3"
                    ]
                },
                "cli": {
                    "inject": [
                        "param.0",
                        "param.1",
                        "options",
                        "cwd"
                    ],
                    "options": {
                        "normalize": [
                            "param.1",
                            "root"
                        ],
                        "boolean": [
                            "get",
                            "use",
                            "pre",
                            "auth",
                            "app"
                        ]
                    }
                },
                "": {
                    "inject": [
                        "param.0",
                        "param.1",
                        "param.2",
                        "param.3"
                    ]
                }
            }
        },
        {
            "name": "require",
            "inject": [],
            "config": {
                "fs": {
                    "path": "dist/commands/require.js",
                    "source": "src/commands/require.ts",
                    "inject": [
                        "ignore",
                        "param.0",
                        "param.1"
                    ]
                },
                "cli": {
                    "inject": [
                        "$injector",
                        "param.0",
                        "cwd"
                    ]
                },
                "": {
                    "inject": [
                        "$injector",
                        "param.0",
                        "param.1"
                    ]
                }
            }
        },
        {
            "name": "webpack",
            "inject": [],
            "config": {
                "fs": {
                    "path": "dist/commands/webpack.js",
                    "source": "src/commands/webpack.ts",
                    "inject": [
                        "param.0",
                        "param.1",
                        "param.2"
                    ]
                },
                "": {
                    "inject": [
                        "param.0",
                        "param.1",
                        "param.2"
                    ]
                },
                "cli": {
                    "options": {
                        "boolean": [
                            "reload"
                        ]
                    },
                    "inject": [
                        "param.0",
                        "options.reload"
                    ]
                }
            }
        },
        {
            "name": "webpack-html",
            "inject": [],
            "config": {
                "fs": {
                    "path": "dist/commands/webpack-html.js",
                    "source": "src/commands/webpack-html.ts",
                    "inject": [
                        "param.0"
                    ]
                },
                "": {
                    "inject": [
                        "param.0"
                    ]
                },
                "cli": {
                    "inject": [
                        "options"
                    ]
                }
            }
        },
        {
            "name": "webpack-alias",
            "inject": [],
            "config": {
                "fs": {
                    "path": "dist/commands/webpack-alias.js",
                    "source": "src/commands/webpack-alias.ts",
                    "inject": [
                        "param.0",
                        "param.1"
                    ]
                },
                "": {
                    "inject": [
                        "param.0",
                        "param.1"
                    ]
                },
                "cli": {
                    "inject": [
                        "param.0",
                        "param.1"
                    ]
                }
            }
        }
    ],
    "$schema": "https://raw.githubusercontent.com/npenin/akala-commands/master/schema.json"
}