{
    "name": "@akala/cron",
    "version": "6.0.8",
    "main": "./dist/cjs/index.js",
    "author": "npenin",
    "license": "BSD-3-Clause",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/npenin/akala.git"
    },
    "source": "src/index.ts",
    "commands": "./commands.json",
    "homepage": "https://akala.js.org/cron",
    "type": "module",
    "scripts": {
        "test": "node --test --enable-source-maps dist/esm/test/**/*.js",
        "coverage": "node --test --enable-source-maps --experimental-test-coverage --test-reporter=lcov --test-reporter-destination=lcov.info --test-reporter=spec --test-reporter-destination=stdout dist/esm/test/**/*.js",
        "generate": "akala sdk generate dist/esm/commands commands.json --name @akala/cron",
        "generate-metadata": "akala sdk generate-metadata commands.json src/container.ts"
    },
    "dependencies": {
        "@akala/commands": "workspace:^",
        "@akala/core": "workspace:^",
        "@akala/pubsub": "workspace:^",
        "debug": "^4.4.1",
        "suncalc": "^1.9.0"
    },
    "devDependencies": {
        "@akala/cli": "workspace:^",
        "@akala/pm": "workspace:^",
        "@types/debug": "^4.1.12"
    },
    "gitHead": "d92f6693ed8cca439c8e92d2045bb15c0a7dae43",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/esm/index.d.ts",
                "default": "./dist/esm/index.js"
            },
            "require": {
                "types": "./dist/cjs/index.d.ts",
                "default": "./dist/cjs/index.js"
            }
        },
        "./commands.json": "./commands.json",
        "./commands/*": "./dist/esm/commands/*.js"
    },
    "module": "./dist/esm/index.js",
    "types": "./dist/esm/index.d.ts",
    "typings": "./dist/esm/index.d.ts"
}
